<section class="itemMain">
    <nav id="sidebar" class="toggled">
        <!-- Sidebar Links -->
        <ul class="menuItems">
            <li>
                <a href="">
                    <i class="fas fa-history"></i> View History</a>
            </li>
            <li>
                <a href="">
                    <i class="fas fa-inbox"></i> Inbox</a>
            </li>
            <li>
                <a href="">
                    <i class="fab fa-wpforms"></i> Forms</a>
            </li>
        </ul>
    </nav>


    <section class="itemInnerMain">
        <div class="container">
            <div class="row">
                <div class="col-md-6 doctorSection">
                    <div class="pillSection">
                        <h2 class="resultHead">View Doctors</h2>
                        <ul class="nav nav-pills nav-fill navBtnHead">
                            {{#each doci}}
                                <li class="nav-item" data-name="{{this}}">
                                    <a class="nav-link navBtnStyle" href="#">{{this}}</a>
                                </li>
                            {{/each}}
                        </ul>
                    </div>
                    <div class="mainDoctors">
                        {{#each doctors}}
                            <div class="individDoctor" data-name="{{this.specialty}}" data-gender="{{this.gender}}">
                                <div class="search-item-doctor-link">
                                    <img class="doctor-image" src="" alt="doctor">
                                    <h4 class="search-item-doctor-name">{{this.name}}</h4>
                                    <button>Book Appointment</button>
                                </div>
                                <div class="star-rating-count" data-rating="{{this.rating}}">
                                    {{this.rating}}
                                </div>
                            </div>
                        {{/each}}
                    </div>
                </div>
                <div class="col-md-6">
                    <h2 class="resultHead">Appointment</h2>
                </div>
            </div>
        </div>
    </section>
</section>

